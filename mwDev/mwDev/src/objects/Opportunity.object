    <sharingModel>Private</sharingModel>
    <validationRules>
        <fullName>Cash_Customer_Name_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>Account_Mannai_Id__c =&apos;CASH CUSTOMER&apos; &amp;&amp; ISBLANK( Cash_Customer_Name__c )</errorConditionFormula>
        <errorMessage>Please fill Cash Customer Name</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_after_Start_Date</fullName>
        <active>true</active>
        <description>The End Date has to be same or later than Start Date</description>
        <errorConditionFormula>Contract_End_Date__c &lt; Contract_Start_Date__c</errorConditionFormula>
        <errorDisplayField>Contract_Start_Date__c</errorDisplayField>
        <errorMessage>End Date cannot be before Start Date!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GM_Percentage</fullName>
        <active>true</active>
        <description>GM% cannot be blank or 0</description>
        <errorConditionFormula>AND(
OR(
ISBLANK(Estimated_GM__c ),
Estimated_GM__c &lt;=0,
Estimated_GM__c &gt;100
),
IsClosed=TRUE)</errorConditionFormula>
        <errorDisplayField>Estimated_GM__c</errorDisplayField>
        <errorMessage>Please fill in an Estimated GM% bigger than 0% and smaller than 100%!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_closed_opportunity_all_fields_Lost</fullName>
        <active>false</active>
        <description>No one can edit anything once deal is closed except from the system administrator.</description>
        <errorConditionFormula>IF( 
CreatedDate &lt;&gt; NOW(), 
AND( 
ISPICKVAL(PRIORVALUE(StageName),&quot;Closed Lost&quot;), 
NOT($Profile.Name=&quot;System Administrator&quot;), 
OR(($User.Id =&quot;005b0000003gFfk&quot;), 
($User.Id=&quot;005b0000003Cuzg&quot;), 
($User.Id=&quot;005b0000001Fn8k&quot;), 
($User.Id=&quot;005b0000003Cuzb&quot;), 
($User.Id=&quot;005b0000001k15j&quot;), 
($User.Id=&quot;005b0000002wgs0&quot;), 
($User.Id=&quot;005b0000003gmps&quot;), 
($User.Id=&quot;005b0000004MOkO&quot;), 
($User.Id=&quot;005b0000004YOxW&quot;), 
($User.Id=&quot;005b0000004YTAe&quot;))), 
AND( 
ISPICKVAL(PRIORVALUE(StageName),&quot;Closed Lost&quot;), 
NOT($Profile.Name&lt;&gt;&quot;System Administrator&quot;)) 
)</errorConditionFormula>
        <errorMessage>You cannot change a deal after the deal is closed. Please contact Administrator</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_closed_opportunity_all_fields_Won</fullName>
        <active>false</active>
        <description>No one can edit anything once deal is closed except from the system administrator.</description>
        <errorConditionFormula>IF( 
CreatedDate &lt;&gt; NOW(), 
AND( 
ISPICKVAL(PRIORVALUE(StageName),&quot;Closed Won&quot;), 
NOT($Profile.Name=&quot;System Administrator&quot;), 
OR(($User.Id =&quot;005b0000003gFfk&quot;), 
($User.Id=&quot;005b0000003Cuzg&quot;), 
($User.Id=&quot;005b0000001Fn8k&quot;), 
($User.Id=&quot;005b0000003Cuzb&quot;), 
($User.Id=&quot;005b0000001k15j&quot;), 
($User.Id=&quot;005b0000002wgs0&quot;), 
($User.Id=&quot;005b0000003gmps&quot;), 
($User.Id=&quot;005b0000004MOkO&quot;), 
($User.Id=&quot;005b0000004YOxW&quot;), 
($User.Id=&quot;005b0000004YTAe&quot;))), 
AND( 
ISPICKVAL(PRIORVALUE(StageName),&quot;Closed Won&quot;), 
NOT($Profile.Name&lt;&gt;&quot;System Administrator&quot;)) 
)</errorConditionFormula>
        <errorMessage>You cannot change a deal after the deal is closed. Please contact Administrator</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_closed_oppostunity</fullName>
        <active>false</active>
        <description>AND( 
ISCHANGED( IsClosed), 
PRIORVALUE( IsClosed), 
NOT( IsClosed), 
NOT($Profile.Name = &quot;System Administrator&quot;))</description>
        <errorConditionFormula>AND( 
ISCHANGED( IsClosed), 
PRIORVALUE( IsClosed), 
NOT( IsClosed), 
$Profile.Name&lt;&gt;&quot;System Administrator&quot;)</errorConditionFormula>
        <errorMessage>You cannot change a deal after the deal is closed. Please contact Administrator</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_lost_opportunities_from_editing</fullName>
        <active>true</active>
        <description>Users can close the opportunity upon creation but cannot change if it is closed. System admins can however, make the required changes.</description>
        <errorConditionFormula>AND(
   $Profile.Name &lt;&gt; &apos;System Administrator&apos;,
   PRIORVALUE(LastModifiedDate) &lt; NOW(),
   OR(
      AND(        
           TEXT(StageName) = &apos;Closed Lost&apos;,
           NOT(ISCHANGED(StageName))
          ),
      AND(
           ISCHANGED(StageName),
           TEXT(PRIORVALUE(StageName)) = &apos;Closed Lost&apos;
         )
     )
  )</errorConditionFormula>
        <errorMessage>You cannot change a closed opportunity. Please contact your administrator.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_won_opportunities_from_editing</fullName>
        <active>true</active>
        <description>Users can close the opportunity upon creation but cannot change if it is closed. System admins can however, make the required changes.</description>
        <errorConditionFormula>AND(
    $Profile.Name &lt;&gt; &apos;System Administrator&apos;,
   PRIORVALUE(LastModifiedDate) &lt; NOW(),
   OR(
      AND(        
           TEXT(StageName) = &apos;Closed Won&apos;,
           NOT(ISCHANGED(StageName))
          ),
      AND(
           ISCHANGED(StageName),
           TEXT(PRIORVALUE(StageName)) = &apos;Closed Won&apos;
         )
     )
  )</errorConditionFormula>
        <errorMessage>You cannot change a closed opportunity. Please contact your administrator.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Overdue_Creation</fullName>
        <active>true</active>
        <description>Open Opportunities are not allowed to have a close date in the past</description>
        <errorConditionFormula>AND(
IsClosed = False,
 CloseDate &lt;  TODAY() ,  $User.Username &lt;&gt; &apos;integration@mannai.com.qa&apos;)</errorConditionFormula>
        <errorDisplayField>CloseDate</errorDisplayField>
        <errorMessage>Please select a Close Date in the future for open Opportunities!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_End_Date</fullName>
        <active>true</active>
        <description>When a deal is closed there has to be a start and end date.</description>
        <errorConditionFormula>AND(
OR(
TEXT( Contract_End_Date__c )=&quot;&quot;,
TEXT( Contract_Start_Date__c )=&quot;&quot;),
TEXT( StageName )=&quot;Closed Won&quot;,
$Profile.Name&lt;&gt;&quot;System Administrator&quot;
)</errorConditionFormula>
        <errorDisplayField>Contract_Start_Date__c</errorDisplayField>
        <errorMessage>Please fill in a Contract Start and End Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_after_close_Date</fullName>
        <active>true</active>
        <description>Contract Start Date cannot be before Close Date.</description>
        <errorConditionFormula>AND(
 Contract_Start_Date__c &lt; CloseDate ,
$Profile.Name &lt;&gt;&quot;System Administrator&quot;)</errorConditionFormula>
        <errorDisplayField>Contract_Start_Date__c</errorDisplayField>
        <errorMessage>Please select a Contract Start Date which is not before the Close Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>closed_date</fullName>
        <active>true</active>
        <description>Can&apos;t change to closed won if 
GM% blank or zero,
no start or end date,
close date in future</description>
        <errorConditionFormula>AND(
IsClosed = TRUE,
CloseDate &gt; TODAY (),
$Profile.Name&lt;&gt;&quot;System Administrator&quot;
)</errorConditionFormula>
        <errorDisplayField>CloseDate</errorDisplayField>
        <errorMessage>The Close Date cannot be in the future.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>closed_lost_past</fullName>
        <active>true</active>
        <description>Closed date can only be in the current month</description>
        <errorConditionFormula>And( 

$Profile.Name&lt;&gt;&quot;System Administrator&quot;, 
ISPICKVAL(StageName, &apos;Closed Lost&apos;), 
or(MONTH( CloseDate ) &lt; MONTH ( TODAY()),YEAR( CloseDate ) &lt; YEAR ( TODAY()))
)</errorConditionFormula>
        <errorDisplayField>CloseDate</errorDisplayField>
        <errorMessage>The Close Date can only be in the current month.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>closed_won_past</fullName>
        <active>true</active>
        <errorConditionFormula>And(

$Profile.Name&lt;&gt;&quot;System Administrator&quot;, 
ISPICKVAL(StageName, &apos;Closed Won&apos;), 
or(MONTH( CloseDate ) &lt; MONTH ( TODAY()),YEAR( CloseDate ) &lt; YEAR ( TODAY())) 

)</errorConditionFormula>
        <errorDisplayField>CloseDate</errorDisplayField>
        <errorMessage>The Close Date can only be in the current month.</errorMessage>
    </validationRules>
</CustomObject>
